{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"], "allowAfterThis": true }],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-use-before-define": ["error", "nofunc"],
    "no-console": "off"
  }

}
